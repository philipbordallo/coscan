  "type": "module",
  "workspaces": [
    "devtools/*",
    "examples/*",
    "packages/*"
  ],
  "packageManager": "npm@10.8.2",
  "dependencies": {},
  "devDependencies": {
    "@dprint/json": "^0.20.0",
    "@dprint/markdown": "^0.19.0",
    "@dprint/typescript": "^0.95.11",
    "@types/node": "^24.7.0",
    "dprint": "^0.50.2",
    "turbo": "^2.5.8",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {},
  "scripts": {
    "build": "turbo run build",
    "start": "turbo run start",
    "clean": "turbo run clean",
