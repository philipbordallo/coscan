{
  "name": "@coscan/json-reporter",
  "version": "0.2.0",
  "author": "Philip Bordallo",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "entry": {
    "main": "./src/main.ts"
  },
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.cjs",
      "types": "./dist/main.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "browserslist": [
    "extends @coscan/browserslist-config"
  ],
  "dependencies": {
    "@coscan/jsx-scanner": "*"
  },
  "devDependencies": {
    "@coscan/rollup": "*"
  },
  "peerDependencies": {},
  "scripts": {
    "build": "rollup -c ./rollup.config.js",
    "build:watch": "rollup -c ./rollup.config.js --watch",
    "clean": "rm -rf .turbo/ && rm -rf dist/ && rm -rf tsconfig.tsbuildinfo",
    "type": "tsc --build --pretty",
    "type:watch": "tsc --build --preserveWatchOutput --watch"
  }
}
